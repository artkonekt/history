{
    "name": "konekt/history",
    "description": "Eloquent Model History for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": ["history", "eloquent", "laravel", "konekt"],
    "support": {
        "source": "https://github.com/artkonekt/history",
        "issues": "https://github.com/artkonekt/history/issues"
    },
    "require": {
        "php": "^8.2",
        "konekt/concord": "^1.13",
        "konekt/enum": "^3.1|^4.0",
        "konekt/enum-eloquent": "^1.9",
        "illuminate/database": "^10.48|^11.46.2|^12.38",
        "illuminate/support": "^10.48|^11.46.2|^12.38",
        "konekt/laravel-migration-compatibility": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Konekt\\History\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\History\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        }
    }
}
